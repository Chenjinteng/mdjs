{
    "version": "0.1.0",
    "isShellCommand": true,
    "showOutput": "silent",
    "echoCommand": true,
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "test",
            "command": "node",
            "args": ["./test/start.js"],
            "isTestCommand": true,
            "problemMatcher": {
                "owner": "javascript",
                "fileLocation": ["relative", "${workspaceRoot}/test/_src"],
                "pattern": {
                    "regexp": "^error: test/generated_test_files/(.+)\\|(.+)\\|(.+)\\|(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            }
        }
    ]
}